{
  "name": "katzen48/laravel-twitch-eventsub",
  "type": "library",
  "description": "This package provides support for subscribing to Twitch EventSub. It's based on laravel-twitch and fully compatible.",
  "require": {
    "php": "^8.0",
    "romanzipp/laravel-twitch": "^4.0.10"
  },
  "authors": [
    {
      "name": "Katzen48",
      "email": "admin@katzen48.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "katzen48\\Twitch\\EventSub\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "katzen48\\Twitch\\EventSub\\Providers\\TwitchEventSubServiceProvider"
      ],
      "aliases": {
        "TwitchEventSub": "katzen48\\Twitch\\EventSub\\Facades\\TwitchEventSub"
      }
    }
  },
  "require-dev": {
    "laravel/framework": "^v8.0"
  }
}
